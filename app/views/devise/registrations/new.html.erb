<% end %>

  <h4 class="line-ting">or</h4>



   
   

    <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= render 'shared/devisemes'%>

      <div class="form-group">
        <%= f.input :fullname, :class => "form-control", :placeholder => "Full Name", label: false %>
      </div>

      <div class="form-group">
        <%= f.input :email, :class => "form-control", :placeholder => "Email", label: false %>
      </div>

      <div class="form-group">
        <%= f.input :password, :class => "form-control", :placeholder => "Password", autocomplete: "off", label: false %>
      </div>

      <div class="field">
        <%= f.input :password_confirmation, :class => "form-control", :placeholder => "Password Confirmation",autocomplete: "off", label: false %>
      </div>

      <br>

      <div class="actions">
        <%= f.submit "Sign up", :class => "btn btn-color button-curve" %>
      </div>
    <% end %>

    

<footer class="or-sign-in">
    Already a member? 
    <%= link_to "Log in", new_session_path(resource_name), class: "member-log-in", data: { turbolinks: false } %><br />
</footer>

  
</div>


<style>
      
      body {
        background: #f5f5f5;
      }

</style>

